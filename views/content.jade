.wrapper
    .container
        .sidebar-left
            .navigation
                .navigation-links
                    .navigation-link-img
                        a(href='#')
                            img(src='/static/images/feed.png')
                    a.navigation-link(href='#') Feed
                .navigation-links
                    .navigation-link-img
                        a(href='#')
                            img(src='/static/images/ask.png')
                    a.navigation-link(href='#') Ask a Colleague
                .navigation-links
                    .navigation-link-img
                        a(href='#')
                            img(src='/static/images/companies.png')
                    a.navigation-link(href='#') Service
                .navigation-links
                    .navigation-link-img
                        a(href='#')
                            img(src='/static/images/services.png')
                    a.navigation-link(href='#') Service Directory
            .advertisement
                .adv-header.adv-bottom Advertisement
                img(src='static/images/three-stars.png')
                a.adv-link(href='#') As By Dentiz.com
            .featured-companies
                .featured-companies-head
                    .head-block Featured companies
                        a.all-link(href='#') See All
                .companies
                    .company.manufacturer
                    .name Company Name
                    .comp-name Manufacturer
                    .location Belgrade, Serbia
                    a.follow(href='#') Follow Now
                .companies
                    .company.service-provider
                    .name Company Name
                    .comp-name Service Provider
                    .location New York, USA
                    a.follow(href='#') Follow Now
                .companies
                    .company.supplier
                    .name Company Name
                    .comp-name Supplier
                    .location London, England
                    a.follow(href='#') Follow Now
            .comp-links
                .comp-link
                    a.adv-link(href='#') Denteez Copyright 2015
                .comp-link
                    a.adv-link(href='#') Terms of use
                    a.adv-link.policy-link(href='#') Privacy Policy
        .category(ng-controller='serviceDirectoryCtrl')
            h1 Service Directory
            button.new-service-btn Add New Service
            ul.services
                li.icon-block(ng-repeat='service in services')
                    a.service-link(href='#')
                        .icon
                            img.icon-img(ng-src='{{ service.icon }}')
                        span.service-name(ng-bind='service.title')
        .sidebar-right
            .dentists
                .head-block People you may know
                    a.all-link(href='#') See All
                .people
                    .name.people-mb Dennis Adams
                    .people-img
                        img.dentist-photo(src='/static/images/adams.png')
                    .dentests-info
                        .comp-name.people-mb Dentist (Fractice Owner)
                        .location.people-mb London, England
                        a.follow(href='#') Add Frienc
                .people
                    .name.people-mb Mary Carpenter
                    .people-img
                        img.dentist-photo(src='/static/images/carpenter.png')
                    .dentests-info
                        .comp-name.people-mb Dentist (Fractice Owner)
                        .location.people-mb Belgrade, Serbia
                        a.follow(href='#') Add Frienc
                .people
                    .name.people-mb Danielle Salazar
                    .people-img
                        img.dentist-photo(src='/static/images/salazar.png')
                    .dentests-info
                        .comp-name.people-mb Dentist (Fractice Owner)
                        .location.people-mb Paris, France
                        a.follow(href='#') Add Frien
            .products
                .head-block Featured Products
                    a.all-link(href='#') See All
                .product
                    .name Product Name
                    .product-img.product-first
                    .product-info-block
                        .product-info Product Short Description.
                        .product-info The quick brown fox jumps over the laze dcg.
                .product
                    .name Product Name
                    .product-img.product-second
                    .product-info-block
                        .product-info Product Short Description.
                        .product-info The quick brown fox jumps over the laze dcg.
            .advertisement
                .adv-header Advertisement
                img(src='static/images/coll.png')
                a.adv-link(href='#') As By Dentiz.com
